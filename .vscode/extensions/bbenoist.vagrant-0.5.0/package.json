{
	"name": "vagrant",
	"displayName": "Vagrant",
	"description": "Vagrant support for Visual Studio Code",
	"version": "0.5.0",
	"publisher": "bbenoist",
	"categories": [
		"Languages",
		"Other"
	],
	"icon": "images/icon.png",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/bbenoist/vscode-vagrant/issues"
	},
	"homepage": "https://github.com/bbenoist/vscode-vagrant/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/bbenoist/vscode-vagrant.git"
	},
	"galleryBanner": {
		"color": "#FFFFFF",
		"theme": "light"
	},
	"engines": {
		"vscode": "^0.10.1"
	},
	"activationEvents": [
		"onCommand:vagrant.up",
		"onCommand:vagrant.provision",
		"onCommand:vagrant.suspend",
		"onCommand:vagrant.halt",
		"onCommand:vagrant.reload",
		"onCommand:vagrant.destroy",
		"onCommand:vagrant.status",
		"onCommand:vagrant.log",
		"workspaceContains:Vagrantfile"
	],
	"main": "./out/src/extension",
	"contributes": {
		"languages": [
			{
				"id": "ruby",
				"extensions": [
					"Vagrantfile"
				]
			}
		],
		"commands": [
			{
				"command": "vagrant.up",
				"title": "Up",
				"category": "Vagrant"
			},
			{
				"command": "vagrant.provision",
				"title": "Provision",
				"category": "Vagrant"
			},
			{
				"command": "vagrant.suspend",
				"title": "Suspend",
				"category": "Vagrant"
			},
			{
				"command": "vagrant.halt",
				"title": "Halt",
				"category": "Vagrant"
			},
			{
				"command": "vagrant.reload",
				"title": "Reload",
				"category": "Vagrant"
			},
			{
				"command": "vagrant.destroy",
				"title": "Destroy",
				"category": "Vagrant"
			},
			{
				"command": "vagrant.status",
				"title": "Status",
				"category": "Vagrant"
			},
			{
				"command": "vagrant.log",
				"title": "Show Log",
				"category": "Vagrant"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"glob": "^7.0.0",
		"rimraf": "^2.5.2",
		"mocha": "^3.2.0",
		"typescript": "^2.2.1",
		"vscode": "^1.1.x",
		"@types/mocha": "^2.2.40",
		"@types/node": "^7.0.10"
	},
	"quickTips": [
		{
			"tip": "The bbenoist.vagrant extension also supports the Vagrant multi-machine feature",
			"url": "https://github.com/bbenoist/vscode-vagrant#vagrant-machine-management"
		},
		{
			"tip": "The bbenoist.vagrant extension can display the status of the machines found in your workspace",
			"url": "https://github.com/bbenoist/vscode-vagrant#vagrant-machine-status"
		}
	],
	"__metadata": {
		"id": "893d887a-97ea-40e6-ae20-860eb6ad582e",
		"publisherId": "787ce942-60d4-48d6-b14e-25bdc72bd145",
		"publisherDisplayName": "Baptist BENOIST"
	}
}