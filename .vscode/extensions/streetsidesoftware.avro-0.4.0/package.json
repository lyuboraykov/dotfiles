{
	"name": "avro",
	"displayName": "avro-idl",
	"description": "Avro IDL Syntax Highlighting",
	"version": "0.4.0",
	"publisher": "streetsidesoftware",
	"engines": {
		"vscode": "^1.14.0"
	},
	"categories": [
		"Languages"
	],
	"qna": "marketplace",
	"icon": "images/avro-logo.png",
	"contributes": {
		"languages": [
			{
				"id": "avro",
				"aliases": [
					"Avro",
					"avro"
				],
				"extensions": [
					".avdl"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "jsonc",
				"extensions": [
					".avpr",
					".avsc"
				]
			}
		],
		"grammars": [
			{
				"language": "avro",
				"scopeName": "source.avro",
				"path": "./node_modules/avro.tmlanguage/avro.tmLanguage.json"
			}
		]
	},
	"dependencies": {
		"avro.tmlanguage": "^1.1.0"
	},
